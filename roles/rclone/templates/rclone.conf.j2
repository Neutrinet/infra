[src]
type = s3
provider = {{ rclone_src_provider | capitalize }}
env_auth = false
access_key_id = {{ rclone_src_access_key }}
secret_access_key = {{ rclone_src_secret_key }}
region = {{ rclone_src_region }}
endpoint = https://{{ rclone_src_host }}
location_constraint =
acl =
storage_class = {{ rclone_src_storage_class }}

[encrypt-backup]
type = crypt
remote = backup:{{ rclone_backup_bucket }}
password = {{ rclone_backup_obscure_encryption_password }}
password2 = {{ rclone_backup_obscure_encryption_salt }}

[backup]
type = s3
provider = {{ rclone_backup_provider | capitalize }}
env_auth = false
access_key_id = {{ rclone_backup_access_key }}
secret_access_key = {{ rclone_backup_secret_key }}
region = {{ rclone_backup_region }}
endpoint = https://{{ rclone_backup_host }}
location_constraint =
acl =
storage_class = {{ rclone_backup_storage_class }}
