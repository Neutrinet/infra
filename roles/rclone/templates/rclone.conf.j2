[src]
type = s3
provider = {{ rclone_src_provider | capitalize }}
env_auth = false
access_key_id = {{ rclone_src_access_key }}
secret_access_key = {{ rclone_src_secret_key }}
region = {{ rclone_src_region }}
endpoint = https://{{ rclone_src_host }}
location_constraint =
acl =
server_side_encryption =
storage_class = {{ rclone_src_storage_class }}

